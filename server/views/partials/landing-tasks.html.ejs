<div class="row">	  
	<% (locals.tasks||[]).forEach( function( task ) { %>
	<div class="col-sm-6 col-md-3 col-lg-2">
		<a href="<%= '/games/' + task.name %>">
	        <div id="<%= 'task-'+task.name %>" align="center">
	            <div class ="highlightit">
	            	<img src="<%= 'assets/games/'+ task.name + '/preview.png' %>"></img>
	            </div>
	            <div align="center">
	                <%= task.displayName %>
	            </div>
	            <script>drawMeritBadges("<%='task-'+task.name %>","<%= task.name %>",3);</script>
	        </div>
        </a>
    </div>
	<% }) %>
</div>